<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="763.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AllObjects.GUI.ChartController">
   <children>
      <LineChart fx:id="lineChart" prefHeight="400.0" prefWidth="500.0" title="Wykres wartości akcji">
        <xAxis>
          <NumberAxis side="BOTTOM" fx:id="x" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="y" side="LEFT" />
        </yAxis>
      </LineChart>
       <Button fx:id="refresh" layoutX="525.0" layoutY="50.0" mnemonicParsing="false" onAction="#refresh" text="Odśwież" />
      <ListView fx:id="listView" layoutX="525.0" layoutY="156.0" prefHeight="200.0" prefWidth="200.0" />
   </children>
</AnchorPane>
