<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="492.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AllObjects.GUI.DisplayTemplates.DetailedView.InvestmentFundDetails">
    <children>
        <Label fx:id="l1" layoutX="50.0" layoutY="100.0" text="Imię" />
