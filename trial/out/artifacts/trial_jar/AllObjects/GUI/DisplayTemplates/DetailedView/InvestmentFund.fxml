        <TableView fx:id="tableView" layoutX="50.0" layoutY="250.0" prefHeight="200.0" prefWidth="300.0">
        </TableView>
        <Button fx:id="deleteItem" layoutX="50.0" layoutY="475.0" mnemonicParsing="false" onAction="#deleteItem" prefWidth="100" text="Usuń" />
